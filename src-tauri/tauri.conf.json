{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "macguffin",
  "version": "0.1.0",
  "identifier": "com.macguffin.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "MacGuffin",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/flickerLogo.ico",
      "icons/flickerLogo.png",
      "icons/flickerLogo.svg"
    ]
  }
}
